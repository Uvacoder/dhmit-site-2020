<header class="header fix top fill-x bg-tan bb z2">
  <div class="header__inner df aic jcb ph10 xs:ph20 pv15">
    <a class="header__logo db" href="#intro" data-router-disabled>
      {% include logo-blue %}
    </a>
    <div class="df aic">
      <div class="df aic m:dn pr10 xs:pr15 z2">
        {% include burger %}
      </div>
      <div class="z1">
        {% include mobile-nav %}
      </div>
      <div class="dn m:db">
        {% include nav %}
      </div>
      <div class="pv10 pl10 xs:pl15 m:pl20 bl">
        <a
          class="header__mit db"
          href="https://mit.edu"
          target="_blank"
          rel="noopener noreferrer"
        >
          {% include mit-logo, primary: '#3D5262', secondary: '#528486' %}
        </a>
      </div>
    </div>
  </div>
</header>